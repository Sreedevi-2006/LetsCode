#Ant on boundary
class Solution:
    def returnToBoundaryCount(self, nums: List[int]) -> int:
        start = 0
        bound = 0
        for i in range(len(nums)):
            start = start + nums[i]
            if start == 0:
                bound = bound+1
        return bound

#avg of even value divisible by 3
class Solution:
    def averageValue(self, nums: List[int]) -> int:
        sum1 = 0
        n = 0
        for i in range(len(nums)):
            if nums[i] % 2 == 0 and nums[i] % 3 == 0:
                sum1 = sum1 + nums[i]
                n = n+1
        if n==0:
            avg = 0
        else:
             avg = sum1//n
        return avg
#nesting of paranthesis
class Solution:
    def maxDepth(self, s: str) -> int:
        ctr=0
        ans=0
        for i in s:
            if i=='(':
                ctr+=1
                ans=max(ans,ctr)
            elif i==')':
                ctr-=1
        return ans
