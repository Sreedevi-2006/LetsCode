#2 sums
class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        op=[]
        for i in range(len(nums)):
            for j in range(i,len(nums)):
                if nums[i]+nums[j] == target:
                    op.append(i,j)
    return op
                    
# minimum number
class Solution:
    def numberGame(self, nums: List[int]) -> List[int]:
        arr = []
        nums.sort()
        for i in range(0,len(nums),2):
            s = nums[i+1]
            r = nums[i]
            arr.append(s)
            arr.append(r)
        return arr
        
